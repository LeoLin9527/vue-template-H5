{
  "name": "@wsim/h5",
  "version": "1.3.0",
  "description": "车间人力信息化系统移动端",
  "author": "180496",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:report": "npm run build --report",
    "inspect": "vue-cli-service inspect --mode production",
    "plop": "plop",
    "svgo": "svgo -f src/icons/svg/svg_svgo --config=src/icons/svgo.yml",
    "commit": "git add . && git-cz",
    "release": "standard-version",
    "tag": "git push --follow-tags origin master"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^3.6.5",
    "echarts": "^4.8.0",
    "fastclick": "^1.0.6",
    "js-cookie": "^2.2.1",
    "normalize.css": "^8.0.1",
    "vant": "^2.8.4",
    "vue": "^2.6.10",
    "vue-count-to": "^1.0.13",
    "vue-router": "^3.3.3",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@vue/cli-plugin-babel": "^4.4.1",
    "@vue/cli-plugin-eslint": "^4.4.1",
    "@vue/cli-plugin-router": "^4.4.1",
    "@vue/cli-plugin-vuex": "^4.4.1",
    "@vue/cli-service": "^4.4.1",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.0",
    "commitizen": "^4.1.2",
    "compression-webpack-plugin": "^4.0.0",
    "cssnano": "^4.1.10",
    "cssnano-preset-advanced": "^4.0.7",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^4.2.5",
    "less": "^3.11.2",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.2.11",
    "plop": "^2.7.1",
    "postcss-aspect-ratio-mini": "^1.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-px-to-viewport": "^1.1.1",
    "postcss-pxtorem": "^5.1.1",
    "postcss-url": "^8.0.0",
    "postcss-viewport-units": "^0.1.6",
    "postcss-write-svg": "^3.0.1",
    "prerender-spa-plugin": "^3.4.0",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "standard-version": "^7.1.0",
    "style-vw-loader": "^1.0.2",
    "svg-sprite-loader": "^4.3.0",
    "svgo": "^1.3.2",
    "vue-skeleton-webpack-plugin": "^1.2.2",
    "vue-template-compiler": "^2.6.10",
    "webpack-bundle-analyzer": "^3.8.0"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "npm run lint",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  }
}
